# generated by ansible

{% if dhcp_subnets is defined %}
{% for item in dhcp_subnets %}	
subnet {{ item.subnet }} netmask {{ item.netmask }} {
 range {{ item.range_min }} {{ item.range_max }};
 option domain-name-servers {{ item.domain_name_server }};
 option domain-name "{{ item.domain_name }}";
 option routers {{ item.router }};
 option broadcast-address {{ item.broadcast_address }};
 default-lease-time {{ item.default_lease_time }};
 max-lease-time {{ item.max_lease_time }};
}
{% endfor %}
{% endif %}  

